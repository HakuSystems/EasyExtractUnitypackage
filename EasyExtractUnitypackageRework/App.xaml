<Application x:Class="EasyExtractUnitypackageRework.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:EasyExtractUnitypackageRework"
             StartupUri="SplashScreen.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="Theme/Buttons/EasyButton.style.xaml"/>
                <ResourceDictionary Source="Theme/Buttons/EasyTextButton.style.xaml"/>
                <ResourceDictionary Source="Theme/ProgressBar/EasyProgressBar.style.xaml"/>
            </ResourceDictionary.MergedDictionaries>
            <BitmapImage x:Key="EasyExtractLogo" UriSource="Theme/Icon - Logo/EasyUnity.png"/>
        </ResourceDictionary>
    </Application.Resources>
</Application>
