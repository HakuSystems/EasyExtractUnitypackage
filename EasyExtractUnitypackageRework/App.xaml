<Application x:Class="EasyExtractUnitypackageRework.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             StartupUri="InitialWindow.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="Theme/Buttons/EasyButton.style.xaml" />
                <ResourceDictionary Source="Theme/Buttons/EasyTextButton.style.xaml" />
                <ResourceDictionary Source="Theme/ProgressBar/EasyProgressBar.style.xaml" />
                <ResourceDictionary
                    Source="pack://application:,,,/MaterialDesignThemes.Wpf;component/Themes/MaterialDesignTheme.Defaults.xaml" />
            </ResourceDictionary.MergedDictionaries>
            <BitmapImage x:Key="EasyExtractLogoBig" UriSource="Theme/Icon - Logo/Big/EEUP_IconBig@4x.png" />
            <BitmapImage x:Key="EasyExtractLogoSmall" UriSource="Theme/Icon - Logo/Small/EEUP_IconSmall@4x.png" />
            <BitmapImage x:Key="EverythingLogo" UriSource="Theme/Icon - Logo/EverythingLogo.png" />
        </ResourceDictionary>
    </Application.Resources>
</Application>