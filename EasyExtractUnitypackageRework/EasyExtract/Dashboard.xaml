<ui:FluentWindow x:Class="EasyExtract.Dashboard"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:local="clr-namespace:EasyExtract"
        xmlns:ui="http://schemas.lepo.co/wpfui/2022/xaml"
        xmlns:gif="https://github.com/XamlAnimatedGif/XamlAnimatedGif"
        mc:Ignorable="d"
        FontFamily="Segoe UI"
        WindowStyle="None"
        ResizeMode="CanResizeWithGrip"
        WindowCornerPreference="Round"
        WindowStartupLocation="CenterScreen"
        Background="Transparent"
        WindowBackdropType="Auto"
        ExtendsContentIntoTitleBar="True"
        Title="EasyExtractUnitypackage" Height="auto" Width="auto">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="*"/>
            <ColumnDefinition Width="*"/>
        </Grid.ColumnDefinitions>
        <Grid.RowDefinitions>
            <RowDefinition Height="30"/>
            <RowDefinition Height="*"/>
            <RowDefinition Height="*"/>
        </Grid.RowDefinitions>
        <ui:TitleBar Foreground="White" Title="EasyExtract" Grid.ColumnSpan="2"></ui:TitleBar>
    </Grid>
</ui:FluentWindow>
