<Window xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:views="using:EasyExtractCrossPlatform"
        xmlns:loc="using:EasyExtractCrossPlatform.Localization"
        Foreground="{DynamicResource EasyTextColorBrush}"
        Background="{DynamicResource EasyWindowBackgroundBrush}"
        mc:Ignorable="d"
        d:DesignWidth="1800"
        d:DesignHeight="1180"
        x:Class="EasyExtractCrossPlatform.MainWindow"
        Title="{loc:Loc MainWindow_Easyextract}"
        Icon="/Assets/logo-flat.ico"
        TransparencyLevelHint="AcrylicBlur"
        TransparencyBackgroundFallback="{DynamicResource EasyWindowBackgroundBrush}">
    <Window.Styles>
        <Style Selector="Border.drop-zone">
            <Setter Property="CornerRadius" Value="24" />
            <Setter Property="Background" Value="{DynamicResource EasySurfaceBrush}" />
            <Setter Property="BorderBrush" Value="{DynamicResource EasyGlassNeutralBorderBrush}" />
            <Setter Property="BorderThickness" Value="1" />
            <Setter Property="Padding" Value="36" />
            <Setter Property="BoxShadow" Value="0 24 40 0 #12000000" />
            <Setter Property="Transitions">
                <Transitions>
                    <BrushTransition Property="{x:Static Border.BackgroundProperty}" Duration="0:0:0.22" />
                    <BrushTransition Property="{x:Static Border.BorderBrushProperty}" Duration="0:0:0.22" />
                    <ThicknessTransition Property="{x:Static Border.BorderThicknessProperty}" Duration="0:0:0.22" />
                </Transitions>
            </Setter>
        </Style>

        <Style Selector="Border.drop-zone.drag-active">
            <Setter Property="Background" Value="{DynamicResource EasyGlassOverlayStrongBrush}" />
            <Setter Property="BorderBrush" Value="{DynamicResource EasyGlassBorderBrush}" />
            <Setter Property="BorderThickness" Value="2" />
        </Style>

        <Style Selector="Border.drop-zone.drag-valid">
            <Setter Property="Background" Value="{DynamicResource EasyAccentSoftBrush}" />
            <Setter Property="BorderBrush" Value="{DynamicResource EasyAccentBrush}" />
            <Setter Property="BorderThickness" Value="2" />
            <Setter Property="Opacity" Value="1" />
        </Style>

        <Style Selector="Border.drop-zone.drag-invalid">
            <Setter Property="Background" Value="{DynamicResource EasyDangerOverlayBrush}" />
            <Setter Property="BorderBrush" Value="{DynamicResource EasyDangerBorderBrush}" />
            <Setter Property="BorderThickness" Value="2" />
            <Setter Property="Opacity" Value="0.85" />
        </Style>

        <Style Selector="Border.drop-zone.drop-success">
            <Setter Property="Background" Value="{DynamicResource EasyAccentStrongBrush}" />
            <Setter Property="BorderBrush" Value="{DynamicResource EasyAccentBrush}" />
            <Setter Property="BorderThickness" Value="2" />
            <Setter Property="Opacity" Value="1" />
        </Style>

        <Style Selector="Grid.page-grid">
            <Setter Property="MaxWidth" Value="1600" />
        </Style>

        <Style Selector="Window.compact Grid.page-grid">
            <Setter Property="Margin" Value="16" />
        </Style>

        <Style Selector="Window.cozy Grid.page-grid">
            <Setter Property="Margin" Value="24" />
        </Style>

        <Style Selector="Window.wide Grid.page-grid">
            <Setter Property="Margin" Value="48" />
        </Style>

        <Style Selector="Window.compact StackPanel.queue-column">
            <Setter Property="Grid.Column" Value="0" />
            <Setter Property="Grid.Row" Value="1" />
            <Setter Property="Margin" Value="0,24,0,0" />
            <Setter Property="HorizontalAlignment" Value="Stretch" />
        </Style>

        <Style Selector="Window.cozy StackPanel.queue-column">
            <Setter Property="Margin" Value="16,0,0,0" />
        </Style>

        <Style Selector="Window.compact Border.drop-zone">
            <Setter Property="Padding" Value="24" />
        </Style>

        <Style Selector="Window.cozy Border.drop-zone">
            <Setter Property="Padding" Value="30" />
        </Style>

        <Style Selector="Window.compact Border.hero-logo">
            <Setter Property="Grid.Column" Value="0" />
            <Setter Property="Grid.Row" Value="0" />
            <Setter Property="HorizontalAlignment" Value="Center" />
            <Setter Property="Margin" Value="0,0,0,16" />
        </Style>

        <Style Selector="Window.compact StackPanel.hero-content">
            <Setter Property="Grid.Column" Value="0" />
            <Setter Property="Grid.Row" Value="1" />
            <Setter Property="HorizontalAlignment" Value="Stretch" />
        </Style>

        <Style Selector="Window.compact StackPanel.hero-action-row">
            <Setter Property="Orientation" Value="Vertical" />
            <Setter Property="Spacing" Value="8" />
        </Style>

        <Style Selector="Window.compact StackPanel.action-row">
            <Setter Property="Orientation" Value="Vertical" />
            <Setter Property="HorizontalAlignment" Value="Stretch" />
            <Setter Property="Spacing" Value="8" />
        </Style>

        <Style Selector="Window.compact StackPanel.action-row Button">
            <Setter Property="HorizontalAlignment" Value="Stretch" />
        </Style>

        <Style Selector="Window.compact StackPanel.footer-primary">
            <Setter Property="Grid.Column" Value="0" />
            <Setter Property="Grid.Row" Value="0" />
            <Setter Property="HorizontalAlignment" Value="Stretch" />
            <Setter Property="Margin" Value="0,0,0,12" />
        </Style>

        <Style Selector="Window.compact StackPanel.footer-center">
            <Setter Property="Grid.Column" Value="0" />
            <Setter Property="Grid.Row" Value="1" />
            <Setter Property="HorizontalAlignment" Value="Center" />
            <Setter Property="Margin" Value="0,0,0,12" />
        </Style>

        <Style Selector="Window.compact StackPanel.footer-secondary">
            <Setter Property="Grid.Column" Value="0" />
            <Setter Property="Grid.Row" Value="2" />
            <Setter Property="Orientation" Value="Vertical" />
            <Setter Property="HorizontalAlignment" Value="Stretch" />
            <Setter Property="Spacing" Value="8" />
        </Style>
    </Window.Styles>

    <Grid>
        <ScrollViewer x:Name="MainScrollViewer"
                      HorizontalScrollBarVisibility="Disabled"
                      VerticalScrollBarVisibility="Auto">
            <Grid x:Name="PageGrid"
                  Classes="page-grid"
                  Margin="40"
                  HorizontalAlignment="Stretch"
                  MaxWidth="1600"
                  RowDefinitions="Auto,*,Auto">
                <Border Grid.Row="0" Margin="0,0,0,32"
                        Padding="28"
                        CornerRadius="28"
                        Background="{DynamicResource EasyGlassGradientBrush}"
                        BorderBrush="{DynamicResource EasyGlassBorderBrush}"
                        BorderThickness="1"
                        BoxShadow="0 40 80 0 #12000000">
                    <Grid x:Name="HeroGrid"
                          ColumnDefinitions="Auto,*"
                          RowDefinitions="Auto,Auto"
                          VerticalAlignment="Top"
                          Classes="hero-grid">
                        <Border Grid.Column="0"
                                Width="128"
                                Height="128"
                                CornerRadius="32"
                                Background="{DynamicResource EasyGlassOverlayBrush}"
                                HorizontalAlignment="Left"
                                VerticalAlignment="Top"
                                BorderBrush="{DynamicResource EasyGlassNeutralBorderBrush}"
                                BorderThickness="1"
                                Margin="0,0,24,0"
                                Classes="hero-logo">
                            <Image Source="avares://EasyExtractCrossPlatform/Assets/AppLogo.png"
                                   Stretch="Uniform"
                                   Margin="12" />
                        </Border>

                        <StackPanel Grid.Column="1"
                                    Classes="hero-content"
                                    Spacing="12"
                                    VerticalAlignment="Top">
                            <TextBlock Text="{loc:Loc MainWindow_Easyextract}"
                                       Classes="display" />
                            <TextBlock Text="{loc:Loc MainWindow_ExtractUnityPackagesWithoutUnity}"
                                       Classes="subtle"
                                       HorizontalAlignment="Left"
                                       MaxWidth="480" />
                            <StackPanel Orientation="Horizontal"
                                        Spacing="12"
                                        Classes="hero-action-row action-row">
                                <Border Padding="12,6"
                                        Background="{DynamicResource EasyGlassOverlayStrongBrush}"
                                        CornerRadius="12">
                                    <StackPanel Orientation="Horizontal"
                                                Spacing="8">
                                        <Ellipse Width="10"
                                                 Height="10"
                                                 Fill="{DynamicResource EasyAccentBrush}"
                                                 VerticalAlignment="Center" />
                                        <TextBlock x:Name="VersionTextBlock"
                                                   Text="{loc:Loc MainWindow_Version0000}"
                                                   VerticalAlignment="Center"
                                                   FontWeight="SemiBold" />
                                    </StackPanel>
                                </Border>
                                <Button x:Name="CheckUpdatesButton"
                                        Classes="ghost"
                                        Click="CheckUpdatesButton_OnClick">
                                    <Button.Content>
                                        <StackPanel Orientation="Horizontal"
                                                    Spacing="8"
                                                    VerticalAlignment="Center">
                                            <PathIcon Data="M12 4A8 8 0 1 0 20 12H18A6 6 0 1 1 12 6V9L16 5L12 1V4Z"
                                                      Width="16"
                                                      Height="16" />
                                            <TextBlock Text="{loc:Loc MainWindow_CheckForUpdates}" />
                                        </StackPanel>
                                    </Button.Content>
                                </Button>
                            </StackPanel>
                            <Border x:Name="UwUModeBanner"
                                    IsVisible="False"
                                    Margin="0,8,0,0"
                                    Padding="12"
                                    CornerRadius="14"
                                    Background="{DynamicResource EasyAccentSoftBrush}"
                                    BorderBrush="{DynamicResource EasyAccentBrush}"
                                    BorderThickness="1">
                                <StackPanel Orientation="Horizontal"
                                            Spacing="10">
                                    <PathIcon
                                        Data="M12 21.35L10.55 20.03C5.4 15.36 2 12.27 2 8.5C2 5.42 4.42 3 7.5 3C9.24 3 10.91 3.81 12 5.09C13.09 3.81 14.76 3 16.5 3C19.58 3 22 5.42 22 8.5C22 12.27 18.6 15.36 13.45 20.04L12 21.35Z"
                                        Width="18"
                                        Height="18"
                                        Foreground="{DynamicResource EasyAccentBrush}"
                                        VerticalAlignment="Center" />
                                    <TextBlock Text="{loc:Loc MainWindow_UwUBanner}"
                                               TextWrapping="Wrap"
                                               FontWeight="SemiBold" />
                                </StackPanel>
                            </Border>
                        </StackPanel>
                    </Grid>
                </Border>

                <Grid Grid.Row="1"
                      x:Name="MainContentGrid"
                      Classes="main-content-grid"
                      ColumnDefinitions="2*,*"
                      RowDefinitions="Auto,Auto"
                      Margin="0,0,0,32">
                    <Border Grid.Column="0"
                            Padding="34"
                            CornerRadius="28"
                            Background="{DynamicResource EasyGlassGradientBrush}"
                            BorderBrush="{DynamicResource EasyGlassBorderBrush}"
                            BorderThickness="1"
                            BoxShadow="0 30 60 0 #14000000">
                        <StackPanel Spacing="28">
                            <Grid x:Name="StartExtractionHeaderGrid"
                                  RowDefinitions="Auto,Auto">
                                <Grid ColumnDefinitions="Auto,*" HorizontalAlignment="Right"
                                      VerticalAlignment="Center">
                                    <StackPanel Orientation="Horizontal"
                                                Spacing="10"
                                                VerticalAlignment="Center">
                                        <PathIcon
                                            Data="M12 3C7.58172 3 4 6.58172 4 11V21H20V11C20 6.58172 16.4183 3 12 3ZM13 17H11V13H9L12 9L15 13H13V17Z"
                                            Width="28"
                                            Height="28"
                                            Foreground="{DynamicResource EasyAccentBrush}"
                                            VerticalAlignment="Center" />
                                        <TextBlock Text="{loc:Loc MainWindow_StartANewExtraction}"
                                                   FontSize="26"
                                                   FontWeight="Bold"
                                                   VerticalAlignment="Center"
                                                   HorizontalAlignment="Center" />
                                    </StackPanel>

                                    <Border x:Name="SearchRevealHost"
                                            Grid.Column="1"
                                            Margin="24,0,0,0"
                                            HorizontalAlignment="Right"
                                            VerticalAlignment="Center"
                                            Padding="0"
                                            PointerEntered="SearchRevealHost_OnPointerEntered"
                                            PointerExited="SearchRevealHost_OnPointerExited">
                                        <Border.Styles>
                                            <Style Selector="Border#SearchRevealHost TextBox.search-bar">
                                                <Setter Property="Width" Value="0" />
                                                <Setter Property="Opacity" Value="0" />
                                                <Setter Property="MinWidth" Value="0" />
                                                <Setter Property="IsHitTestVisible" Value="False" />
                                                <Setter Property="Margin" Value="8,0,0,0" />
                                            </Style>
                                            <Style Selector="Border#SearchRevealHost:pointerover TextBox.search-bar">
                                                <Setter Property="Width" Value="280" />
                                                <Setter Property="Opacity" Value="1" />
                                                <Setter Property="IsHitTestVisible" Value="True" />
                                            </Style>
                                            <Style Selector="Border#SearchRevealHost.search-active TextBox.search-bar">
                                                <Setter Property="Width" Value="280" />
                                                <Setter Property="Opacity" Value="1" />
                                                <Setter Property="IsHitTestVisible" Value="True" />
                                            </Style>
                                        </Border.Styles>
                                        <Grid ColumnDefinitions="Auto,Auto">
                                            <StackPanel Spacing="8" HorizontalAlignment="Right">
                                                <Border x:Name="SearchHintContainer"
                                                        Background="{DynamicResource EasyGlassOverlayStrongBrush}"
                                                        BorderBrush="{DynamicResource EasyAccentSoftBrush}"
                                                        BorderThickness="1"
                                                        CornerRadius="16"
                                                        Padding="10,6"
                                                        HorizontalAlignment="Right"
                                                        Opacity="0"
                                                        IsHitTestVisible="False">
                                                    <Border.Transitions>
                                                        <Transitions>
                                                            <DoubleTransition Property="Opacity"
                                                                              Duration="0:0:0.18"
                                                                              Easing="0,0.55,0.45,1" />
                                                        </Transitions>
                                                    </Border.Transitions>
                                                    <TextBlock
                                                        Text="{loc:Loc MainWindow_SearchYourDeviceForUnitypackageFiles}"
                                                        Classes="subtle"
                                                        HorizontalAlignment="Right" />
                                                </Border>
                                                <Grid ColumnDefinitions="Auto,*" HorizontalAlignment="Right"
                                                      VerticalAlignment="Center">
                                                    <Border x:Name="SearchIconBorder"
                                                            Width="44"
                                                            Height="44"
                                                            CornerRadius="22"
                                                            Background="{DynamicResource EasyGlassOverlayStrongBrush}"
                                                            BorderBrush="{DynamicResource EasyGlassNeutralBorderBrush}"
                                                            BorderThickness="1"
                                                            HorizontalAlignment="Center"
                                                            VerticalAlignment="Center">
                                                        <PathIcon
                                                            Data="M15.5 14H14.71L14.43 13.73C15.41 12.59 16 11.11 16 9.5C16 5.91 13.09 3 9.5 3C5.91 3 3 5.91 3 9.5C3 13.09 5.91 16 9.5 16C11.11 16 12.59 15.41 13.73 14.43L14 14.71V15.5L18 19.49L19.49 18L15.5 14ZM9.5 14C7.01 14 5 11.99 5 9.5C5 7.01 7.01 5 9.5 5C11.99 5 14 7.01 14 9.5C14 11.99 11.99 14 9.5 14Z"
                                                            Width="20"
                                                            Height="20"
                                                            Foreground="{DynamicResource EasyAccentBrush}"
                                                            HorizontalAlignment="Center"
                                                            VerticalAlignment="Center" />
                                                    </Border>
                                                    <TextBox x:Name="UnityPackageSearchBox"
                                                             Grid.Column="1"
                                                             Margin="12,0,0,0"
                                                             Text="{Binding SearchQuery, UpdateSourceTrigger=PropertyChanged}"
                                                             Classes="search-bar search-bar-small"
                                                             x:CompileBindings="False" />
                                                </Grid>
                                            </StackPanel>
                                        </Grid>
                                    </Border>
                                </Grid>
                            </Grid>

                            <Grid x:Name="DropZoneHostGrid">
                                <Border x:Name="DropZoneBorder"
                                        Classes="drop-zone"
                                        DragDrop.AllowDrop="True"
                                        DragDrop.DragEnter="DropZoneBorder_OnDragEnter"
                                        DragDrop.DragLeave="DropZoneBorder_OnDragLeave"
                                        DragDrop.Drop="DropZoneBorder_OnDrop"
                                        DragDrop.DragOver="DropZoneBorder_OnDragOver">
                                    <StackPanel Spacing="22" HorizontalAlignment="Center">
                                        <Border Width="88" Height="88"
                                                CornerRadius="28"
                                                Background="{DynamicResource EasyGlassOverlayFaintBrush}"
                                                BorderBrush="{DynamicResource EasyGlassNeutralBorderBrush}"
                                                BorderThickness="1"
                                                HorizontalAlignment="Center">
                                            <PathIcon Data="M21 20V18H3V20H21Z M12 2L4 10H9V17H15V10H20L12 2Z"
                                                      Width="34"
                                                      Height="34"
                                                      Foreground="{DynamicResource EasyAccentBrush}"
                                                      HorizontalAlignment="Center"
                                                      VerticalAlignment="Center" />
                                        </Border>

                                        <StackPanel Spacing="6" HorizontalAlignment="Center">
                                            <TextBlock x:Name="DropZonePrimaryTextBlock"
                                                       Text="{loc:Loc MainWindow_DragAmpDropFilesHere}"
                                                       FontSize="20"
                                                       FontWeight="SemiBold"
                                                       TextAlignment="Center" />
                                            <TextBlock x:Name="DropZoneSecondaryTextBlock"
                                                       Text="{loc:Loc MainWindow_SupportsBatchExtractionAndLiveProgress}"
                                                       Classes="subtle"
                                                       TextAlignment="Center" />
                                        </StackPanel>

                                        <StackPanel Orientation="Horizontal"
                                                    Spacing="10"
                                                    HorizontalAlignment="Center"
                                                    Classes="action-row">
                                            <Button x:Name="StartExtractionButton"
                                                    Width="160"
                                                    Click="StartExtractionButton_OnClick">
                                                <Button.Content>
                                                    <StackPanel Orientation="Horizontal" Spacing="8"
                                                                HorizontalAlignment="Center">
                                                        <PathIcon
                                                            Data="M3 6H8L10 8H20C21.1046 8 22 8.89543 22 10V18C22 19.1046 21.1046 20 20 20H4C2.89543 20 2 19.1046 2 18V7C2 6.44772 2.44772 6 3 6Z"
                                                            Width="18"
                                                            Height="18"
                                                            Foreground="{DynamicResource EasyAccentBrush}"
                                                            VerticalAlignment="Center" />
                                                        <TextBlock Text="{loc:Loc MainWindow_StartExtraction}"
                                                                   VerticalAlignment="Center" />
                                                    </StackPanel>
                                                </Button.Content>
                                            </Button>
                                            <Button x:Name="BatchExtractionButton"
                                                    Classes="ghost"
                                                    Click="BatchExtractionButton_OnClick">
                                                <Button.Content>
                                                    <StackPanel Orientation="Horizontal" Spacing="8"
                                                                VerticalAlignment="Center">
                                                        <PathIcon
                                                            Data="M3 6H8L10 8H20C21.1046 8 22 8.89543 22 10V18C22 19.1046 21.1046 20 20 20H4C2.89543 20 2 19.1046 2 18V7C2 6.44772 2.44772 6 3 6Z M16 11V13H19V15H16V18H14V15H11V13H14V11H16Z"
                                                            Width="18"
                                                            Height="18"
                                                            Foreground="{DynamicResource EasyAccentBrush}"
                                                            VerticalAlignment="Center" />
                                                        <TextBlock Text="{loc:Loc MainWindow_BatchExtraction}"
                                                                   VerticalAlignment="Center" />
                                                    </StackPanel>
                                                </Button.Content>
                                            </Button>
                                            <Button x:Name="OpenOutputFolderButton"
                                                    Classes="ghost"
                                                    Click="OpenOutputFolderButton_OnClick">
                                                <Button.Content>
                                                    <StackPanel Orientation="Horizontal" Spacing="8"
                                                                VerticalAlignment="Center">
                                                        <PathIcon
                                                            Data="M19 19H5V5H19V19ZM19 3H5C3.89 3 3 3.9 3 5V19C3 20.1 3.89 21 5 21H19C20.11 21 21 20.11 21 19V5C21 3.9 20.11 3 19 3ZM10 17L15 12L10 7V17Z"
                                                            Width="18"
                                                            Height="18"
                                                            Foreground="{DynamicResource EasyAccentBrush}"
                                                            VerticalAlignment="Center" />
                                                        <TextBlock Text="{loc:Loc MainWindow_OpenOutputFolder}"
                                                                   VerticalAlignment="Center" />
                                                    </StackPanel>
                                                </Button.Content>
                                            </Button>
                                        </StackPanel>
                                    </StackPanel>
                                </Border>

                                <Border x:Name="SearchResultsBorder"
                                        Margin="0"
                                        Padding="24"
                                        Background="{DynamicResource EasyGlassOverlayStrongBrush}"
                                        BorderBrush="{DynamicResource EasyGlassNeutralBorderBrush}"
                                        BorderThickness="1"
                                        CornerRadius="24"
                                        Opacity="0"
                                        IsVisible="False"
                                        IsHitTestVisible="False">
                                    <views:EverythingSearchView x:Name="EverythingSearch"
                                                                Classes="compact minimal"
                                                                HorizontalAlignment="Stretch"
                                                                VerticalAlignment="Stretch" />
                                </Border>
                            </Grid>

                            <Border x:Name="ExtractionDashboard"
                                    Padding="28"
                                    CornerRadius="24"
                                    Background="{DynamicResource EasyGlassGradientBrush}"
                                    BorderBrush="{DynamicResource EasyAccentSoftBrush}"
                                    BorderThickness="1"
                                    BoxShadow="0 26 60 0 #18000000"
                                    Opacity="0"
                                    IsVisible="False">
                                <Border.Transitions>
                                    <Transitions>
                                        <DoubleTransition Property="Opacity"
                                                          Duration="0:0:0.25"
                                                          Easing="0.2,0.8,0.2,1" />
                                    </Transitions>
                                </Border.Transitions>
                                <Border.RenderTransform>
                                    <ScaleTransform ScaleX="0.98" ScaleY="0.98">
                                        <ScaleTransform.Transitions>
                                            <Transitions>
                                                <DoubleTransition Property="{x:Static ScaleTransform.ScaleXProperty}"
                                                                  Duration="0:0:0.25"
                                                                  Easing="0.2,0.8,0.2,1" />
                                                <DoubleTransition Property="{x:Static ScaleTransform.ScaleYProperty}"
                                                                  Duration="0:0:0.25"
                                                                  Easing="0.2,0.8,0.2,1" />
                                            </Transitions>
                                        </ScaleTransform.Transitions>
                                    </ScaleTransform>
                                </Border.RenderTransform>
                                <StackPanel Spacing="18">
                                    <Grid ColumnDefinitions="Auto,*,Auto,Auto" VerticalAlignment="Center">
                                        <Border Grid.Column="0"
                                                Width="40" Height="40" CornerRadius="20"
                                                Background="{DynamicResource EasyGlassOverlayStrongBrush}"
                                                BorderBrush="{DynamicResource EasyAccentBrush}"
                                                BorderThickness="1">
                                            <PathIcon
                                                Data="M12 2C6.477 2 2 6.477 2 12C2 17.523 6.477 22 12 22C17.523 22 22 17.523 22 12H19.5C19.5 16.1421 16.1421 19.5 12 19.5C7.85786 19.5 4.5 16.1421 4.5 12C4.5 7.85786 7.85786 4.5 12 4.5C14.0196 4.5 15.8645 5.28121 17.1905 6.60998L14.5 9.25H22V2L19.1757 4.82426C17.4006 3.04502 14.8484 2 12 2Z"
                                                Width="20"
                                                Height="20"
                                                Foreground="{DynamicResource EasyAccentBrush}"
                                                HorizontalAlignment="Center"
                                                VerticalAlignment="Center" />
                                        </Border>
                                        <StackPanel Grid.Column="1" Margin="12,0,0,0">
                                            <TextBlock Text="{loc:Loc MainWindow_ExtractionInProgress}"
                                                       FontSize="18"
                                                       FontWeight="SemiBold" />
                                            <TextBlock x:Name="ExtractionDashboardSubtitle"
                                                       Text="{loc:Loc MainWindow_PreparingPackage}"
                                                       Classes="subtle" />
                                        </StackPanel>
                                        <Border Grid.Column="2"
                                                Margin="12,0,0,0"
                                                HorizontalAlignment="Right"
                                                VerticalAlignment="Center"
                                                Padding="10,4"
                                                CornerRadius="12"
                                                Background="{DynamicResource EasyGlassOverlayStrongBrush}"
                                                BorderBrush="{DynamicResource EasyAccentSoftBrush}"
                                                BorderThickness="1">
                                            <TextBlock x:Name="ExtractionDashboardQueueCount"
                                                       Text="{loc:Loc MainWindow_Queue0}"
                                                       FontWeight="SemiBold"
                                                       Foreground="{DynamicResource EasyAccentBrush}" />
                                        </Border>
                                        <Button x:Name="CancelExtractionButton"
                                                Grid.Column="3"
                                                Margin="12,0,0,0"
                                                HorizontalAlignment="Right"
                                                VerticalAlignment="Center"
                                                Classes="ghost"
                                                IsVisible="False"
                                                IsEnabled="False"
                                                Click="CancelExtractionButton_OnClick">
                                            <Button.Content>
                                                <StackPanel Orientation="Horizontal"
                                                            Spacing="6"
                                                            HorizontalAlignment="Center">
                                                    <PathIcon
                                                        Data="M6 6H18V18H6Z"
                                                        Width="18"
                                                        Height="18"
                                                        Foreground="{DynamicResource EasyDangerForegroundBrush}"
                                                        VerticalAlignment="Center" />
                                                    <TextBlock Text="{loc:Loc MainWindow_Stop}"
                                                               VerticalAlignment="Center"
                                                               Foreground="{DynamicResource EasyDangerForegroundBrush}" />
                                                </StackPanel>
                                            </Button.Content>
                                        </Button>
                                    </Grid>
                                    <Border x:Name="ExtractionDashboardSecurityBanner"
                                            Padding="14"
                                            CornerRadius="16"
                                            Margin="0,6,0,0"
                                            Background="{DynamicResource EasyGlassOverlayFaintBrush}"
                                            BorderBrush="{DynamicResource EasyGlassNeutralBorderBrush}"
                                            BorderThickness="1"
                                            IsVisible="False">
                                        <StackPanel Orientation="Horizontal" Spacing="10">
                                            <PathIcon
                                                Data="M11 2L1 20H21L11 2ZM11 7.75L16.25 17H5.75L11 7.75ZM10 12V15H12V12H10ZM10 16V18H12V16H10Z"
                                                Width="22"
                                                Height="22"
                                                Foreground="{DynamicResource EasyWarningBrush}"
                                                VerticalAlignment="Center" />
                                            <TextBlock x:Name="ExtractionDashboardSecurityText"
                                                       TextWrapping="Wrap"
                                                       FontWeight="SemiBold"
                                                       FontSize="15"
                                                       VerticalAlignment="Center"
                                                       Text="Potentially malicious package detected." />
                                        </StackPanel>
                                    </Border>

                                    <Grid ColumnDefinitions="Auto,*" RowDefinitions="Auto,Auto">
                                        <ProgressBar x:Name="ExtractionDashboardProgressBar"
                                                     Grid.ColumnSpan="2"
                                                     Height="12"
                                                     Classes="accent"
                                                     IsIndeterminate="True" />
                                        <TextBlock Grid.Row="1"
                                                   Grid.Column="0"
                                                   Text="{loc:Loc MainWindow_CurrentPackage}"
                                                   Classes="subtle"
                                                   Margin="0,12,0,0" />
                                        <TextBlock x:Name="ExtractionDashboardPackageText"
                                                   Grid.Row="1"
                                                   Grid.Column="1"
                                                   Text="ΓÇö"
                                                   Margin="12,12,0,0"
                                                   FontWeight="SemiBold"
                                                   TextTrimming="CharacterEllipsis"
                                                   HorizontalAlignment="Left" />
                                    </Grid>

                                    <StackPanel Spacing="12">
                                        <Grid ColumnDefinitions="Auto,*" RowDefinitions="Auto,Auto">
                                            <TextBlock Text="{loc:Loc MainWindow_Asset}"
                                                       Classes="subtle" />
                                            <TextBlock x:Name="ExtractionDashboardAssetText"
                                                       Grid.Column="1"
                                                       Text="{loc:Loc MainWindow_Waiting}"
                                                       Margin="12,0,0,0"
                                                       TextTrimming="CharacterEllipsis" />
                                            <TextBlock Grid.Row="1"
                                                       Text="{loc:Loc MainWindow_Output}"
                                                       Classes="subtle"
                                                       Margin="0,8,0,0" />
                                            <TextBlock x:Name="ExtractionDashboardOutputText"
                                                       Grid.Row="1"
                                                       Grid.Column="1"
                                                       Text="ΓÇö"
                                                       Margin="12,8,0,0"
                                                       TextTrimming="CharacterEllipsis" />
                                        </Grid>
                                        <Border CornerRadius="18"
                                                BorderThickness="1"
                                                BorderBrush="{DynamicResource EasyGlassNeutralBorderBrush}"
                                                Background="{DynamicResource EasyGlassOverlayFaintBrush}"
                                                Padding="16">
                                            <Grid ColumnDefinitions="*,*,*">
                                                <StackPanel Grid.Column="0"
                                                            Margin="0,0,16,0">
                                                    <TextBlock Text="{loc:Loc MainWindow_AssetsExtracted}"
                                                               Classes="subtle" />
                                                    <TextBlock x:Name="ExtractionDashboardAssetCount"
                                                               Text="0"
                                                               FontSize="22"
                                                               FontWeight="SemiBold" />
                                                </StackPanel>
                                                <StackPanel Grid.Column="1"
                                                            Margin="0,0,16,0">
                                                    <TextBlock Text="{loc:Loc MainWindow_ElapsedTime}"
                                                               Classes="subtle" />
                                                    <TextBlock x:Name="ExtractionDashboardElapsed"
                                                               Text="{loc:Loc MainWindow_0s}"
                                                               FontSize="22"
                                                               FontWeight="SemiBold" />
                                                </StackPanel>
                                                <StackPanel Grid.Column="2">
                                                    <TextBlock Text="{loc:Loc MainWindow_NextUp}"
                                                               Classes="subtle" />
                                                    <TextBlock x:Name="ExtractionDashboardNextPackage"
                                                               Text="{loc:Loc MainWindow_AllCaughtUp}"
                                                               FontSize="22"
                                                               FontWeight="SemiBold"
                                                               TextTrimming="CharacterEllipsis" />
                                                </StackPanel>
                                            </Grid>
                                        </Border>
                                    </StackPanel>
                                </StackPanel>
                            </Border>
                        </StackPanel>
                    </Border>

                    <StackPanel Grid.Column="1"
                                Classes="queue-column"
                                Spacing="28"
                                Margin="28,0,0,0">
                        <Border Padding="28"
                                CornerRadius="24"
                                Background="{DynamicResource EasyGlassGradientBrush}"
                                BorderBrush="{DynamicResource EasyGlassBorderBrush}"
                                BorderThickness="1"
                                BoxShadow="0 26 50 0 #14000000">
                            <StackPanel Spacing="18">
                                <Grid ColumnDefinitions="Auto,*,Auto" VerticalAlignment="Center">
                                    <Border Grid.Column="0"
                                            Width="36" Height="36" CornerRadius="18"
                                            Margin="0,0,12,0"
                                            Background="{DynamicResource EasyGlassOverlayBrush}"
                                            BorderBrush="{DynamicResource EasyGlassNeutralBorderBrush}"
                                            BorderThickness="1">
                                        <PathIcon
                                            Data="M5 4H19C20.1046 4 21 4.89543 21 6V18C21 19.1046 20.1046 20 19 20H5C3.89543 20 3 19.1046 3 18V6C3 4.89543 3.89543 4 5 4Z M7 8H17V10H7V8Z M7 12H17V14H7V12Z M7 16H14V18H7V16Z"
                                            Width="20"
                                            Height="20"
                                            Foreground="{DynamicResource EasyAccentBrush}"
                                            VerticalAlignment="Center" />
                                    </Border>
                                    <StackPanel Grid.Column="1">
                                        <TextBlock Text="{loc:Loc MainWindow_ExtractionQueue}"
                                                   FontWeight="SemiBold"
                                                   FontSize="18" />
                                        <TextBlock x:Name="QueueSummaryTextBlock"
                                                   Text="{loc:Loc MainWindow_QueueIsEmpty}"
                                                   Classes="subtle" />
                                    </StackPanel>
                                    <StackPanel Grid.Column="2"
                                                Orientation="Horizontal"
                                                Spacing="8"
                                                HorizontalAlignment="Right">
                                        <Button x:Name="ProcessQueueButton"
                                                Click="ProcessQueueButton_OnClick"
                                                ToolTip.Tip="{loc:Loc MainWindow_ExtractEveryPackageCurrentlyQueued}"
                                                Content="{loc:Loc MainWindow_ExtractQueue}" />
                                        <Button x:Name="ClearQueueButton"
                                                Classes="ghost"
                                                Click="ClearQueueButton_OnClick"
                                                ToolTip.Tip="{loc:Loc MainWindow_RemoveEveryPackageFromTheQueue}"
                                                Content="{loc:Loc MainWindow_ClearQueue}" />
                                    </StackPanel>
                                </Grid>

                                <StackPanel x:Name="QueueEmptyState"
                                            Spacing="6"
                                            HorizontalAlignment="Center"
                                            VerticalAlignment="Center">
                                    <TextBlock Text="{loc:Loc MainWindow_NoPackagesInQueueYet}"
                                               FontWeight="SemiBold"
                                               TextAlignment="Center" />
                                    <TextBlock
                                        Text="{loc:Loc MainWindow_DropUnitypackageFilesOrAddThem}"
                                        Classes="subtle"
                                        TextAlignment="Center"
                                        MaxWidth="320" />
                                </StackPanel>

                                <ScrollViewer x:Name="QueueItemsScrollViewer"
                                              MaxHeight="320"
                                              IsVisible="False"
                                              HorizontalScrollBarVisibility="Disabled"
                                              VerticalScrollBarVisibility="Auto">
                                    <ItemsControl x:Name="QueueItemsControl">
                                        <ItemsControl.ItemsPanel>
                                            <ItemsPanelTemplate>
                                                <StackPanel Spacing="12" />
                                            </ItemsPanelTemplate>
                                        </ItemsControl.ItemsPanel>
                                        <ItemsControl.ItemTemplate>
                                            <DataTemplate x:CompileBindings="False">
                                                <Border CornerRadius="18"
                                                        Background="{DynamicResource EasyGlassOverlayFaintBrush}"
                                                        BorderBrush="{DynamicResource EasyGlassNeutralBorderBrush}"
                                                        BorderThickness="1"
                                                        Padding="16">
                                                    <Grid ColumnDefinitions="*,Auto,Auto">
                                                        <StackPanel Spacing="4">
                                                            <TextBlock Text="{Binding FileName}"
                                                                       FontWeight="SemiBold"
                                                                       TextTrimming="CharacterEllipsis" />
                                                            <TextBlock Text="{Binding LocationText}"
                                                                       Classes="subtle"
                                                                       TextTrimming="CharacterEllipsis"
                                                                       ToolTip.Tip="{Binding FilePath}" />
                                                            <TextBlock Text="{Binding SecurityWarningText}"
                                                                       Margin="0,2,0,0"
                                                                       Classes="subtle"
                                                                       Foreground="{DynamicResource EasyWarningBrush}"
                                                                       TextTrimming="CharacterEllipsis"
                                                                       IsVisible="{Binding HasSecurityWarning}" />
                                                            <TextBlock Text="{Binding SecurityInfoText}"
                                                                       Margin="0,2,0,0"
                                                                       Classes="subtle"
                                                                       Foreground="{DynamicResource EasyGlassMutedBrush}"
                                                                       TextTrimming="CharacterEllipsis"
                                                                       IsVisible="{Binding HasSecurityInfo}" />
                                                        </StackPanel>
                                                        <StackPanel Grid.Column="1"
                                                                    Margin="16,0,0,0"
                                                                    Spacing="4"
                                                                    HorizontalAlignment="Right">
                                                            <TextBlock Text="{Binding SizeText}"
                                                                       FontWeight="SemiBold"
                                                                       HorizontalAlignment="Right" />
                                                            <TextBlock Text="{Binding StatusText}"
                                                                       Classes="subtle"
                                                                       HorizontalAlignment="Right" />
                                                        </StackPanel>
                                                        <Button Grid.Column="2"
                                                                Content="{loc:Loc MainWindow_Preview}"
                                                                Classes="ghost"
                                                                Margin="16,0,0,0"
                                                                Click="PreviewQueueItemButton_OnClick"
                                                                HorizontalAlignment="Right"
                                                                VerticalAlignment="Center"
                                                                ToolTip.Tip="{loc:Loc MainWindow_InspectThePackageWithoutExtracting}" />
                                                    </Grid>
                                                </Border>
                                            </DataTemplate>
                                        </ItemsControl.ItemTemplate>
                                    </ItemsControl>
                                </ScrollViewer>
                            </StackPanel>
                        </Border>
                        <Border Padding="28"
                                CornerRadius="24"
                                Background="{DynamicResource EasyGlassGradientBrush}"
                                BorderBrush="{DynamicResource EasyGlassBorderBrush}"
                                BorderThickness="1"
                                BoxShadow="0 26 50 0 #14000000">
                            <StackPanel Spacing="18">
                                <StackPanel Orientation="Horizontal" Spacing="12" VerticalAlignment="Center">
                                    <Border Width="36" Height="36" CornerRadius="18"
                                            Background="{DynamicResource EasyGlassOverlayBrush}"
                                            BorderBrush="{DynamicResource EasyGlassNeutralBorderBrush}"
                                            BorderThickness="1">
                                        <Image Source="avares://EasyExtractCrossPlatform/Assets/AppBadge.png"
                                               Stretch="Uniform"
                                               Margin="6" />
                                    </Border>
                                    <TextBlock Text="{loc:Loc MainWindow_ExtractionOverview}"
                                               FontWeight="SemiBold"
                                               FontSize="18" />
                                </StackPanel>
                                <Separator Margin="0,4" />
                                <Grid RowDefinitions="Auto,Auto,Auto" ColumnDefinitions="*,Auto">
                                    <TextBlock Text="{loc:Loc MainWindow_PackagesCompleted}"
                                               Classes="subtle" />
                                    <TextBlock x:Name="ExtractionOverviewPackagesCompletedText"
                                               Grid.Column="1"
                                               Text="0"
                                               FontSize="26"
                                               FontWeight="Bold"
                                               Foreground="{DynamicResource EasyAccentBrush}" />

                                    <TextBlock Grid.Row="1"
                                               Classes="subtle"
                                               Margin="0,16,0,0"
                                               Text="{loc:Loc MainWindow_AssetsExtracted}" />
                                    <TextBlock x:Name="ExtractionOverviewAssetsExtractedText"
                                               Grid.Row="1"
                                               Grid.Column="1"
                                               Margin="0,16,0,0"
                                               Text="0"
                                               FontSize="26"
                                               FontWeight="Bold"
                                               Foreground="{DynamicResource EasyGlassMutedBrush}" />

                                    <TextBlock Grid.Row="2"
                                               Classes="subtle"
                                               Margin="0,16,0,0"
                                               Text="{loc:Loc MainWindow_LastExtraction}" />
                                    <TextBlock x:Name="ExtractionOverviewLastExtractionText"
                                               Grid.Row="2"
                                               Grid.Column="1"
                                               Margin="0,16,0,0"
                                               Text="{loc:Loc MainWindow_NotStarted}"
                                               FontWeight="SemiBold" />
                                </Grid>
                            </StackPanel>
                        </Border>

                    </StackPanel>
                </Grid>

                <Border Grid.Row="2"
                        CornerRadius="22"
                        Padding="18"
                        Background="{DynamicResource EasyGlassOverlayFaintBrush}"
                        BorderBrush="{DynamicResource EasyGlassNeutralBorderBrush}"
                        BorderThickness="1"
                        BoxShadow="0 20 40 0 #12000000">
                    <Grid x:Name="FooterGrid"
                          ColumnDefinitions="Auto,*,Auto"
                          RowDefinitions="Auto"
                          VerticalAlignment="Center"
                          Classes="footer-grid">
                        <StackPanel Orientation="Horizontal"
                                    Spacing="12"
                                    VerticalAlignment="Center"
                                    Classes="footer-primary">
                            <Button Classes="ghost"
                                    ToolTip.Tip="{loc:Loc MainWindow_OpenDashboard}"
                                    Click="DashboardButton_OnClick">
                                <Button.Content>
                                    <StackPanel Orientation="Horizontal" Spacing="6">
                                        <PathIcon
                                            Data="M3 3H11V11H3V3ZM3 13H11V21H3V13ZM13 3H21V11H13V3ZM13 13H21V21H13V13Z"
                                            Width="16"
                                            Height="16"
                                            Foreground="{DynamicResource EasyAccentBrush}" />
                                        <TextBlock Text="{loc:Loc MainWindow_WebDashboard}"
                                                   VerticalAlignment="Center" />
                                    </StackPanel>
                                </Button.Content>
                            </Button>
                            <Button Classes="ghost"
                                    ToolTip.Tip="{loc:Loc MainWindow_ViewHistory}"
                                    Click="HistoryButton_OnClick">
                                <Button.Content>
                                    <StackPanel Orientation="Horizontal" Spacing="6">
                                        <PathIcon
                                            Data="M4 19H20V21H4V19ZM6 15H10V7H6V15ZM11 15H15V3H11V15ZM16 15H20V11H16V15Z"
                                            Width="18"
                                            Height="18"
                                            Foreground="{DynamicResource EasyAccentBrush}" />
                                        <TextBlock Text="{loc:Loc MainWindow_History}"
                                                   VerticalAlignment="Center" />
                                    </StackPanel>
                                </Button.Content>
                            </Button>
                            <Button Classes="ghost"
                                    ToolTip.Tip="{loc:Loc MainWindow_SendFeedback}"
                                    Click="FeedbackButton_OnClick">
                                <Button.Content>
                                    <StackPanel Orientation="Horizontal" Spacing="6">
                                        <PathIcon
                                            Data="M10.75,14 C11.9926407,14 13,15.0073593 13,16.25 L13,17.7519766 L12.9921156,17.8604403 C12.6813607,19.9866441 10.7715225,21.0090369 7.5667905,21.0090369 C4.37361228,21.0090369 2.43330141,19.9983408 2.01446278,17.8965776 L2,17.75 L2,16.25 C2,15.0073593 3.00735931,14 4.25,14 L10.75,14 Z M10.75,15.5 L4.25,15.5 C3.83578644,15.5 3.5,15.8357864 3.5,16.25 L3.5,17.670373 C3.77979024,18.870787 5.05062598,19.5090369 7.5667905,19.5090369 C10.082858,19.5090369 11.2966388,18.8777026 11.5,17.6931543 L11.5,16.25 C11.5,15.8357864 11.1642136,15.5 10.75,15.5 Z M7.5,6 C9.43299662,6 11,7.56700338 11,9.5 C11,11.4329966 9.43299662,13 7.5,13 C5.56700338,13 4,11.4329966 4,9.5 C4,7.56700338 5.56700338,6 7.5,6 Z M19.75,2 C20.9926407,2 22,3.00735931 22,4.25 L22,7.75 C22,8.99264069 20.9926407,10 19.75,10 L18.2951017,10 L16.1285541,12.1414295 C15.637473,12.6266415 14.846031,12.6218837 14.360819,12.1308026 C14.1296416,11.8968289 14,11.5811661 14,11.2526232 L13.9993676,9.98619781 C12.8746845,9.86154927 12,8.90792139 12,7.75 L12,4.25 C12,3.00735931 13.0073593,2 14.25,2 L19.75,2 Z M7.5,7.5 C6.3954305,7.5 5.5,8.3954305 5.5,9.5 C5.5,10.6045695 6.3954305,11.5 7.5,11.5 C8.6045695,11.5 9.5,10.6045695 9.5,9.5 C9.5,8.3954305 8.6045695,7.5 7.5,7.5 Z M19.75,3.5 L14.25,3.5 C13.8357864,3.5 13.5,3.83578644 13.5,4.25 L13.5,7.75 C13.5,8.16421356 13.8357864,8.5 14.25,8.5 L16,8.5 L16,10.5 L18.2512219,8.25634766 L19.75,8.25 C20.1642136,8.25 20.5,7.91421356 20.5,7.5 L20.5,4.25 C20.5,3.83578644 20.1642136,3.5 19.75,3.5 Z"
                                            Width="18" Height="18" />
                                        <TextBlock Text="{loc:Loc MainWindow_Feedback}" />
                                    </StackPanel>
                                </Button.Content>
                            </Button>
                        </StackPanel>

                        <StackPanel Grid.Column="1"
                                    Orientation="Horizontal"
                                    Spacing="6"
                                    HorizontalAlignment="Center"
                                    VerticalAlignment="Center"
                                    Classes="footer-center">
                            <PathIcon
                                Data="M12 21.35L10.55 20.03C5.4 15.36 2 12.27 2 8.5C2 5.42 4.42 3 7.5 3C9.24 3 10.91 3.81 12 5.09C13.09 3.81 14.76 3 16.5 3C19.58 3 22 5.42 22 8.5C22 12.27 18.6 15.36 13.45 20.04L12 21.35Z"
                                Width="18"
                                Height="18"
                                Foreground="{DynamicResource EasyAccentBrush}"
                                VerticalAlignment="Center" />
                            <TextBlock Text="{loc:Loc MainWindow_MadeByHakusystems}"
                                       VerticalAlignment="Center" />
                        </StackPanel>

                        <StackPanel Grid.Column="2"
                                    Orientation="Horizontal"
                                    Spacing="12"
                                    HorizontalAlignment="Right"
                                    Classes="footer-secondary">
                            <Button Classes="ghost"
                                    ToolTip.Tip="{loc:Loc MainWindow_ViewCredits}"
                                    Click="CreditsButton_OnClick">
                                <Button.Content>
                                    <StackPanel Orientation="Horizontal"
                                                Spacing="6">
                                        <PathIcon
                                            Data="M16 11C17.66 11 18.99 9.66 18.99 8C18.99 6.34 17.66 5 16 5C14.34 5 13 6.34 13 8C13 9.66 14.34 11 16 11ZM8 11C9.66 11 10.99 9.66 10.99 8C10.99 6.34 9.66 5 8 5C6.34 5 5 6.34 5 8C5 9.66 6.34 11 8 11ZM8 13C5.33 13 0 14.34 0 17V19H16V17C16 14.34 10.67 13 8 13ZM19 13C18.18 13 17.45 13.08 16.8 13.21C17.9 14.02 18.66 15.15 18.66 16.5V19H24V17C24 14.34 21.33 13 19 13Z"
                                            Width="18"
                                            Height="18"
                                            Foreground="{DynamicResource EasyAccentBrush}" />
                                        <TextBlock Text="{loc:Loc MainWindow_Credits}"
                                                   VerticalAlignment="Center" />
                                    </StackPanel>
                                </Button.Content>
                            </Button>
                            <Button Classes="ghost"
                                    ToolTip.Tip="{loc:Loc MainWindow_OpenSettings}"
                                    Click="SettingsButton_OnClick">
                                <Button.Content>
                                    <PathIcon
                                        Data="M14 9.50006C11.5147 9.50006 9.5 11.5148 9.5 14.0001C9.5 16.4853 11.5147 18.5001 14 18.5001C15.3488 18.5001 16.559 17.9066 17.3838 16.9666C18.0787 16.1746 18.5 15.1365 18.5 14.0001C18.5 13.5401 18.431 13.0963 18.3028 12.6784C17.7382 10.8381 16.0253 9.50006 14 9.50006ZM11 14.0001C11 12.3432 12.3431 11.0001 14 11.0001C15.6569 11.0001 17 12.3432 17 14.0001C17 15.6569 15.6569 17.0001 14 17.0001C12.3431 17.0001 11 15.6569 11 14.0001Z M21.7093 22.3948L19.9818 21.6364C19.4876 21.4197 18.9071 21.4515 18.44 21.7219C17.9729 21.9924 17.675 22.4693 17.6157 23.0066L17.408 24.8855C17.3651 25.273 17.084 25.5917 16.7055 25.682C14.9263 26.1061 13.0725 26.1061 11.2933 25.682C10.9148 25.5917 10.6336 25.273 10.5908 24.8855L10.3834 23.0093C10.3225 22.4731 10.0112 21.9976 9.54452 21.7281C9.07783 21.4586 8.51117 21.4269 8.01859 21.6424L6.29071 22.4009C5.93281 22.558 5.51493 22.4718 5.24806 22.1859C4.00474 20.8536 3.07924 19.2561 2.54122 17.5137C2.42533 17.1384 2.55922 16.7307 2.8749 16.4977L4.40219 15.3703C4.83721 15.0501 5.09414 14.5415 5.09414 14.0007C5.09414 13.4598 4.83721 12.9512 4.40162 12.6306L2.87529 11.5051C2.55914 11.272 2.42513 10.8638 2.54142 10.4882C3.08038 8.74734 4.00637 7.15163 5.24971 5.82114C5.51684 5.53528 5.93492 5.44941 6.29276 5.60691L8.01296 6.36404C8.50793 6.58168 9.07696 6.54881 9.54617 6.27415C10.0133 6.00264 10.3244 5.52527 10.3844 4.98794L10.5933 3.11017C10.637 2.71803 10.9245 2.39704 11.3089 2.31138C12.19 2.11504 13.0891 2.01071 14.0131 2.00006C14.9147 2.01047 15.8128 2.11485 16.6928 2.31149C17.077 2.39734 17.3643 2.71823 17.4079 3.11017L17.617 4.98937C17.7116 5.85221 18.4387 6.50572 19.3055 6.50663C19.5385 6.507 19.769 6.45838 19.9843 6.36294L21.7048 5.60568C22.0626 5.44818 22.4807 5.53405 22.7478 5.81991C23.9912 7.1504 24.9172 8.74611 25.4561 10.487C25.5723 10.8623 25.4386 11.2703 25.1228 11.5035L23.5978 12.6297C23.1628 12.95 22.9 13.4586 22.9 13.9994C22.9 14.5403 23.1628 15.0489 23.5988 15.3698L25.1251 16.4965C25.441 16.7296 25.5748 17.1376 25.4586 17.5131C24.9198 19.2536 23.9944 20.8492 22.7517 22.1799C22.4849 22.4657 22.0671 22.5518 21.7093 22.3948Z"
                                        Width="18" Height="18" />
                                </Button.Content>
                            </Button>
                        </StackPanel>
                    </Grid>
                </Border>
            </Grid>
        </ScrollViewer>

        <Border x:Name="OverlayHost"
                Background="#CC050505"
                IsVisible="False"
                IsHitTestVisible="False"
                Opacity="0">
            <Border.Transitions>
                <Transitions>
                    <DoubleTransition Property="{x:Static Visual.OpacityProperty}"
                                      Duration="0:0:0.24" />
                </Transitions>
            </Border.Transitions>
            <Grid>
                <Border x:Name="OverlayCard"
                        HorizontalAlignment="Center"
                        VerticalAlignment="Center"
                        MaxWidth="920"
                        MaxHeight="680"
                        Margin="32"
                        Background="{DynamicResource EasyWindowBackgroundBrush}"
                        BorderBrush="{DynamicResource EasyGlassBorderBrush}"
                        BorderThickness="1"
                        CornerRadius="36"
                        ClipToBounds="True"
                        Padding="0"
                        BoxShadow="0 48 120 0 #22000000"
                        RenderTransformOrigin="0.5,0.5">
                    <Border.RenderTransform>
                        <ScaleTransform ScaleX="0.94"
                                        ScaleY="0.94">
                            <ScaleTransform.Transitions>
                                <Transitions>
                                    <DoubleTransition Property="{x:Static ScaleTransform.ScaleXProperty}"
                                                      Duration="0:0:0.24" />
                                    <DoubleTransition Property="{x:Static ScaleTransform.ScaleYProperty}"
                                                      Duration="0:0:0.24" />
                                </Transitions>
                            </ScaleTransform.Transitions>
                        </ScaleTransform>
                    </Border.RenderTransform>
                    <ContentControl x:Name="OverlayContent" />
                </Border>
            </Grid>
        </Border>
    </Grid>
</Window>